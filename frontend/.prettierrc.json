{
  "singleQuote": true,
  "semi": true,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "printWidth": 100,
  "endOfLine": "lf",
  "importOrder": [
    "^(react)",
    "^(@)",
    "^((?!/).)*$",
    "^((.*)/types)(.*|$)",
    "^((.*)/hooks)(.*|$)",
    "^((.*)/utils)(.*|$)",
    "^(common/components)*$",
    "^(common/components)(.*|$)",
    "^(service/(.*)/components)(.*|$)",
    "^(pages)(.*|$)",
    "^((assets|constants))(.*|$)",
    "^(./styles.module)(.*|$)",
    "^(.)(.*|$)",
    "^(..)(.*|$)"
  ],
  "importOrderSeparation": true
}
