{
  "singleQuote": true,
  "semi": true,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "printWidth": 100,
  "endOfLine": "lf",
  "importOrder": [
    "^react",
    "^@",
    "<THIRD_PARTY_MODULES>",
    "^((?!/).)*$",
    "^((.*)/types)(.*|$)",
    "^((.*)/hooks)(.*|$)",
    "^((.*)/utils)(.*|$)",
    "^routes(.*|$)",
    "^common/components(.*|$)",
    "^service/components(.*|$)",
    "^pages(.*|$)",
    "^((assets|constants))(.*|$)",
    "^((.|..)/(?!styles))(.*|$)",
    "^(./styles.module)(.*|$)"
  ],
  "importOrderSeparation": true
}
